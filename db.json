lass=\"hljs-variable\">$point</span> == large-up {\n    <span class=\"hljs-keyword\">@media</span> (<span class=\"hljs-attribute\">min-width</span>: <span class=\"hljs-number\">75em</span>) {\n      <span class=\"hljs-comment\">// 1200px</span>\n      <span class=\"hljs-keyword\">@content</span>;\n    }\n  } <span class=\"hljs-keyword\">@else</span> {\n    <span class=\"hljs-keyword\">@media</span> (<span class=\"hljs-attribute\">min-width</span>: ((<span class=\"hljs-variable\">$point</span> / <span class=\"hljs-number\">16</span>) * <span class=\"hljs-number\">1em</span>)) {\n      <span class=\"hljs-keyword\">@content</span>;\n    }\n  }\n}",
